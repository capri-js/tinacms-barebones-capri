{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "target": "ES2017",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "jsx": "preserve"
  },
  "include": ["src", ".tina"]
}
